{
    "name": "inverse-search",
    "displayName": "Inverse Search",
    "description": "select everything in document except the searched word",
    "version": "0.0.2",
    "publisher": "ctf0",
    "repository": "https://github.com/ctf0/vscode-inverse-search",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:inverse.search"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "inverse.search",
                "title": "Inverse Search"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "inverse.search",
                    "when": "resourceScheme =~ /(file|untitled)/"
                }
            ]
        },
        "configuration": {
            "title": "Inverse Search",
            "properties": {
                "inverse_search.includeStart": {
                    "type": "boolean",
                    "default": false,
                    "description": "include text before first match"
                },
                "inverse_search.includeEnd": {
                    "type": "boolean",
                    "default": false,
                    "description": "include text after last match"
                }
            }
        }
    },
    "scripts": {
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.41.0",
        "eslint": "^6.6.0",
        "glob": "^7.1.5",
        "mocha": "^6.2.2",
        "typescript": "^3.6.4",
        "vscode-test": "^1.2.2"
    },
    "dependencies": {
        "escape-string-regexp": "^2.0.0"
    }
}
